{
  "name": "osu-track",
  "version": "0.4.0",
  "description": "Track osu! top plays and do other things",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "dev:pm2": "pm2 start ecosystem.config.js --env development --watch && pm2 log osu-tracking",
    "start": "node src/index.js",
    "start:pm2": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop osu-tracking",
    "lint": "standard",
    "fix": "standard --fix"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.18.1",
    "axios": "^0.21.1",
    "cron": "^1.8.2",
    "discord-buttons": "^4.0.0",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "node-osu": "^2.2.1",
    "ordr.js": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "nodemon": "^2.0.12",
    "standard": "^16.0.3"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "engines": {
    "node": ">=14.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KeziahMoselle/osu-track"
  },
  "author": "KeziahMoselle",
  "license": "MIT"
}
