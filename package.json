{
  "name": "osu-tracking",
  "version": "1.0.0",
  "description": "Tracking osu! top plays",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "dev:pm2": "pm2 start ecosystem.config.js --env development --watch && pm2 log osu-tracking",
    "start": "node src/index.js",
    "start:pm2": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop osu-tracking"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.18.0",
    "axios": "^0.21.1",
    "cron": "^1.8.2",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "node-osu": "^2.2.1",
    "ordr.js": "^1.0.0",
    "pm2": "^5.1.0"
  },
  "engines": {
    "node": ">=14.8.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  },
  "author": "KeziahMoselle",
  "license": "MIT"
}
